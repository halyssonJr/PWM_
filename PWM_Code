#include <avr/io.h>
#include <avr/interrupt.h>
#include <util/delay.h>
#define F_CPU 16000000

int main(void){

  // direção dos pino do registrador, pode subistitur B por A
  // habilitar como input na porta 13
  //         0x20
  //        Carregar 1 para o bit 5
  DDRB  |= (1 << 0);
  // Operação OR
  // DDRB =DDRB | (1<<5)

  // inicia o PORTB em zero

  PORTB &= ~(1 << 0);


  while (1)
  {
    PORTB |= (1 << 0);
    _delay_ms(1000);
    PORTB &= ~(1 << 0);
    _delay_ms(1000);
  }




}
